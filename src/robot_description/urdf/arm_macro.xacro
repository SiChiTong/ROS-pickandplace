			</collision>
			<inertial>
       		  <mass value="10.0"/>
        		<origin xyz="-0.000265 -0.022912 -0.263484" rpy="0 0 0"/>
        		<inertia ixx="0.112042" ixy="-9.49E-06" ixz="5.84E-06" iyy="0.026163" iyz="-0.022776" izz="0.110175"/>
			</inertial>
